<%= render "shared/header" %>
<div id="page">
	<div id="content">
		<h1>search a venue</h1>
		<%=form_tag do %>
		enter location:<%=search_field_tag 'location'%><br/>
		enter search:<%=search_field_tag 'query'%><br/>
		<%=submit_tag 'submit your search'%>

		<%end%>
		<div>
			<%=@location%>
			<%=@query%>
		</div>
		
	</div>
</div>